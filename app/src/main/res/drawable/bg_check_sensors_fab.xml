<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">

            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <solid android:color="?attr/bgColorFabIcon" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1.5dp"
                android:color="?attr/fillColorFabIcon" />
            <size
                android:width="50dp"
                android:height="50dp" />
        </shape>
    </item>

</layer-list>